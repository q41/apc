<?xml version="1.0" encoding="ASCII"?>
<bp:Root xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bp="http://www.emftext.org/language/ballandpaddle">
  <level id="first" powerSpawnChance="1.0">
    <blocks>.......</blocks>
    <blocks>...A...</blocks>
    <blocks>..BAB..</blocks>
    <blocks>.......</blocks>
    <blocks>.......</blocks>
  </level>
  <paddles id="Paddle" x="4.0" y="4.0" size="10"/>
  <balls id="Ball" x="3.0" y="4.0" size="10" orientation="270" damage="1" speed="1"/>
  <blocks id="A" hardness="1" power="//@powers.0"/>
  <blocks id="B" hardness="1"/>
  <effects xsi:type="bp:GeneralEffect" id="LessSpeedB">
    <target xsi:type="bp:ClassTarget" classType="ball"/>
    <body name="damage">
      <expression xsi:type="bp:IntOperand" value="-3"/>
    </body>
  </effects>
  <effects xsi:type="bp:GeneralEffect" id="LessSpeedP">
    <target xsi:type="bp:ClassTarget" classType="paddle"/>
    <body>
      <expression xsi:type="bp:IntOperand" value="-30"/>
    </body>
  </effects>
  <powers id="Speed" effects="//@effects.0 //@effects.1"/>
</bp:Root>
