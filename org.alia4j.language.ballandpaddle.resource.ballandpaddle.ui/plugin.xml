	<extension point="org.eclipse.debug.ui.launchConfigurationTypeImages">
		<launchConfigurationTypeImage icon="/icons/launch_type_icon.gif" configTypeID="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.launchConfigurationType" id="org.alia4j.language.ballandpaddle.launchImage">
		</launchConfigurationTypeImage>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
		<launchConfigurationTabGroup type="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.launchConfigurationType" class="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.launch.BallandpaddleLaunchConfigurationTabGroup" id="org.alia4j.language.ballandpaddle.launchConfigurationTabGroup" helpContextId="org.alia4j.language.ballandpaddle.launchConfigHelpContext">
			<launchMode mode="debug" perspective="org.eclipse.debug.ui.DebugPerspective" description="Debug ballandpaddle model.">
			</launchMode>
			<launchMode mode="run" description="Run ballandpaddle model.">
			</launchMode>
		</launchConfigurationTabGroup>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchShortcuts">
		<shortcut label="ballandpaddle Application" icon="/icons/launch_shortcut_icon.gif" helpContextId="org.alia4j.language.ballandpaddle.launch" modes="run,debug" class="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.launch.BallandpaddleLaunchShortcut" description="Launch a ballandpaddle model" id="org.alia4j.language.ballandpaddle.launchShortcut">
			<description description="Run ballandpaddle model" mode="run">
			</description>
			<description description="Debug ballandpaddle model" mode="debug">
			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="org.alia4j.language.ballandpaddle.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
			<configurationType id="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.launchConfigurationType">
			</configurationType>
		</shortcut>
	</extension>
	
	<extension point="org.eclipse.core.expressions.propertyTesters">
		<propertyTester id="org.alia4j.language.ballandpaddle.PropertyTester" type="java.lang.Object" namespace="org.alia4j.language.ballandpaddle" properties="isLaunchable" class="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.BallandpaddlePropertyTester">
		</propertyTester>
	</extension>
	
