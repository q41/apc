		<markerResolutionGenerator class="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.BallandpaddleMarkerResolutionGenerator" markerType="org.alia4j.language.ballandpaddle.resource.ballandpaddle.problem">
		</markerResolutionGenerator>
	</extension>
	
	<extension point="org.eclipse.core.filebuffers.annotationModelCreation">
		<factory class="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.BallandpaddleAnnotationModelFactory" extensions="ballandpaddle">
		</factory>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.alia4j.language.ballandpaddle.resource.ballandpaddle" name=".ballandpaddle File" base-type="org.eclipse.core.runtime.text" file-extensions="ballandpaddle">
		</content-type>
	</extension>
	
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider class="org.eclipse.ui.editors.text.TextFileDocumentProvider" extensions="ballandpaddle" id="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.provider">
