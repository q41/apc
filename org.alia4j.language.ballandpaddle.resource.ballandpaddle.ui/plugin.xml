		<category id="org.emftext.runtime.ui.EMFTextProjectCategory" name="EMFText Project">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextProjectCategory" icon="/icons/default_new_icon.gif" class="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.BallandpaddleNewProjectWizard" id="org.alia4j.language.ballandpaddle.resource.ballandpaddle.ui.BallandpaddleNewProjectWizard" name="EMFText ballandpaddle project" project="true">
		</wizard>
