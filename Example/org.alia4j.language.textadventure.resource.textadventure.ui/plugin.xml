		<markerResolutionGenerator class="org.alia4j.language.textadventure.resource.textadventure.ui.TextadventureMarkerResolutionGenerator" markerType="org.alia4j.language.textadventure.resource.textadventure.problem">
		</markerResolutionGenerator>
	</extension>
	
	<extension point="org.eclipse.core.filebuffers.annotationModelCreation">
		<factory class="org.alia4j.language.textadventure.resource.textadventure.ui.TextadventureAnnotationModelFactory" extensions="textadventure">
		</factory>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.alia4j.language.textadventure.resource.textadventure" name=".textadventure File" base-type="org.eclipse.core.runtime.text" file-extensions="textadventure">
		</content-type>
	</extension>
	
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider class="org.eclipse.ui.editors.text.TextFileDocumentProvider" extensions="textadventure" id="org.alia4j.language.textadventure.resource.textadventure.ui.provider">
		</provider>
	</extension>
	
	<extension point="org.eclipse.ui.editors.annotationTypes">
		<type name="org.alia4j.language.textadventure.resource.textadventure.ui.occurences">
		</type>
		<type name="org.alia4j.language.textadventure.resource.textadventure.ui.occurences.declaration" super="org.alia4j.language.textadventure.resource.textadventure.ui.occurences">
		</type>
	</extension>
	
