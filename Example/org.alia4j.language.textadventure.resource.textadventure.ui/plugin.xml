		</wizard>
		<category id="org.emftext.runtime.ui.EMFTextProjectCategory" name="EMFText Project">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextProjectCategory" icon="/icons/default_new_icon.gif" class="org.alia4j.language.textadventure.resource.textadventure.ui.TextadventureNewProjectWizard" id="org.alia4j.language.textadventure.resource.textadventure.ui.TextadventureNewProjectWizard" name="EMFText textadventure project" project="true">
		</wizard>
	</extension>
	
