		<specification annotationType="org.alia4j.language.textadventure.resource.textadventure.ui.occurences" label="Occurrences (in .textadventure files)" icon="/icons/occurrence_icon.gif" textPreferenceKey="textadventure.occurrenceIndication" textPreferenceValue="false" highlightPreferenceKey="textadventure.occurrenceHighlighting" highlightPreferenceValue="true" contributesToHeader="false" overviewRulerPreferenceKey="textadventure.occurrenceIndicationInOverviewRuler" overviewRulerPreferenceValue="true" verticalRulerPreferenceKey="textadventure.occurrenceIndicationInVerticalRuler" verticalRulerPreferenceValue="false" colorPreferenceKey="textadventure.occurrenceIndicationColor" colorPreferenceValue="212,212,212" presentationLayer="4" showInNextPrevDropdownToolbarActionKey="textadventure.showOccurrenceInNextPrevDropdownToolbarAction" showInNextPrevDropdownToolbarAction="true" isGoToNextNavigationTargetKey="textadventure.isOccurrenceGoToNextNavigationTarget" isGoToNextNavigationTarget="false" isGoToPreviousNavigationTargetKey="textadventure.isOccurrenceGoToPreviousNavigationTarget" isGoToPreviousNavigationTarget="false" textStylePreferenceKey="textadventure.occurrenceTextStyle" textStylePreferenceValue="NONE">
