	<extension point="org.eclipse.debug.ui.launchConfigurationTypeImages">
		<launchConfigurationTypeImage icon="/icons/launch_type_icon.gif" configTypeID="org.alia4j.language.textadventure.resource.textadventure.ui.launchConfigurationType" id="org.alia4j.language.textadventure.launchImage">
		</launchConfigurationTypeImage>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchConfigurationTabGroups">
		<launchConfigurationTabGroup type="org.alia4j.language.textadventure.resource.textadventure.ui.launchConfigurationType" class="org.alia4j.language.textadventure.resource.textadventure.ui.launch.TextadventureLaunchConfigurationTabGroup" id="org.alia4j.language.textadventure.launchConfigurationTabGroup" helpContextId="org.alia4j.language.textadventure.launchConfigHelpContext">
			<launchMode mode="debug" perspective="org.eclipse.debug.ui.DebugPerspective" description="Debug textadventure model.">
			</launchMode>
			<launchMode mode="run" description="Run textadventure model.">
			</launchMode>
		</launchConfigurationTabGroup>
	</extension>
	
	<extension point="org.eclipse.debug.ui.launchShortcuts">
		<shortcut label="textadventure Application" icon="/icons/launch_shortcut_icon.gif" helpContextId="org.alia4j.language.textadventure.launch" modes="run,debug" class="org.alia4j.language.textadventure.resource.textadventure.ui.launch.TextadventureLaunchShortcut" description="Launch a textadventure model" id="org.alia4j.language.textadventure.launchShortcut">
			<description description="Run textadventure model" mode="run">
			</description>
			<description description="Debug textadventure model" mode="debug">
			</description>
			<contextualLaunch>
				<enablement>
					<with variable="selection">
						<count value="1">
						</count>
						<iterate>
							<test property="org.alia4j.language.textadventure.isLaunchable">
							</test>
						</iterate>
					</with>
				</enablement>
			</contextualLaunch>
			<configurationType id="org.alia4j.language.textadventure.resource.textadventure.ui.launchConfigurationType">
			</configurationType>
		</shortcut>
	</extension>
	
	<extension point="org.eclipse.core.expressions.propertyTesters">
		<propertyTester id="org.alia4j.language.textadventure.PropertyTester" type="java.lang.Object" namespace="org.alia4j.language.textadventure" properties="isLaunchable" class="org.alia4j.language.textadventure.resource.textadventure.ui.TextadventurePropertyTester">
		</propertyTester>
	</extension>
	
	<extension point="org.eclipse.ui.editors.annotationTypes">
		<type name="org.alia4j.language.textadventure.resource.textadventure.ui.debug.lineBreakpoint" markerType="org.alia4j.language.textadventure.resource.textadventure.debug.lineBreakpoint.marker" super="org.eclipse.debug.core.breakpoint">
		</type>
	</extension>
	
