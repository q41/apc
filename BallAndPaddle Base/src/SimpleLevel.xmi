<?xml version="1.0" encoding="ASCII"?>
<ballandpaddle:Root xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ballandpaddle="http://www.alia4j.org/language/ballandpaddle">
  <level id="first">
    <blocks>.......</blocks>
    <blocks>...A...</blocks>
    <blocks>..BAB..</blocks>
	<blocks>.......</blocks>
	<blocks>.......</blocks>
  </level>
  <paddles id="Paddle" x="2.0" y="4.0" size="1"/>
  <balls id="Ball" x="3.0" y="4.0"/>
  <blocks id="A" hardness="2" coldRes="2" shockRes="3"/>
  <blocks id="B" hardness="1" power="//@powers.0"/>
  <effects id="LaunchBall" type="speed">
    <target xsi:type="ballandpaddle:ThisTarget" item="this">
      <params xsi:type="ballandpaddle:AndParameter">
        <left xsi:type="ballandpaddle:SmthParameter">
          <left xsi:type="ballandpaddle:AttParameter" att="speed"/>
          <right xsi:type="ballandpaddle:IntValueParameter" value="5"/>
        </left>
        <right xsi:type="ballandpaddle:LthParameter">
          <left xsi:type="ballandpaddle:AttParameter" att="speed"/>
          <right xsi:type="ballandpaddle:IntValueParameter"/>
        </right>
      </params>
    </target>
    <expression xsi:type="ballandpaddle:PlusExpression">
      <left xsi:type="ballandpaddle:AttOperand" att="speed"/>
      <right xsi:type="ballandpaddle:IntOperand" value="2"/>
    </expression>
  </effects>
  <powers id="TestPower" effects="//@effects.0">
    <target xsi:type="ballandpaddle:ObjectTarget" item="//@balls.0"/>
  </powers>
</ballandpaddle:Root>
