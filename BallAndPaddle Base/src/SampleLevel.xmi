<?xml version="1.0" encoding="ASCII"?>
<ballandpaddle:Root xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ballandpaddle="http://www.alia4j.org/language/ballandpaddle">
  <level id="first">
    <blocks>...AAA...</blocks>
    <blocks>...BBB...</blocks>
    <blocks>...CCC...</blocks>
  </level>
  <paddles id="Paddle" x="4" y="4" size="1"/>
  <balls id="Ball" x="3" y="4"/>
  <blocks id="A" hardness="1" coldRes="2" shockRes="3"/>
  <blocks id="B" hardness="5" fireRes="3" coldRes="1" shockRes="1" power="//@powers.0"/>
  <effects id="MoreBalls" type="normal damage">
    <target xsi:type="ballandpaddle:TypeTarget" type="ball">
      <params xsi:type="ballandpaddle:OrParameter">
        <left xsi:type="ballandpaddle:NeqParameter">
          <left xsi:type="ballandpaddle:AttParameter" att="y"/>
          <right xsi:type="ballandpaddle:ValueParameter" value="2"/>
        </left>
        <right xsi:type="ballandpaddle:BracketParameter">
          <body xsi:type="ballandpaddle:AndParameter">
            <left xsi:type="ballandpaddle:EqParameter">
              <left xsi:type="ballandpaddle:AttParameter"/>
              <right xsi:type="ballandpaddle:ValueParameter" value="4"/>
            </left>
            <right xsi:type="ballandpaddle:LthParameter">
              <left xsi:type="ballandpaddle:AttParameter" att="y"/>
              <right xsi:type="ballandpaddle:AttParameter"/>
            </right>
          </body>
        </right>
      </params>
    </target>
    <expression xsi:type="ballandpaddle:MultExpression">
      <left xsi:type="ballandpaddle:DivExpression">
        <left xsi:type="ballandpaddle:BracketExpression">
          <body xsi:type="ballandpaddle:PlusExpression">
            <left xsi:type="ballandpaddle:AttOperand" att="speed"/>
            <right xsi:type="ballandpaddle:IntOperand" value="3"/>
          </body>
        </left>
        <right xsi:type="ballandpaddle:IntOperand" value="5"/>
      </left>
      <right xsi:type="ballandpaddle:IntOperand" value="2"/>
    </expression>
  </effects>
  <effects id="MoreSpeed" type="hardness">
    <target xsi:type="ballandpaddle:ObjectTarget" item="//@blocks.1"/>
    <expression xsi:type="ballandpaddle:MultExpression">
      <left xsi:type="ballandpaddle:AttOperand" att="hardness"/>
      <right xsi:type="ballandpaddle:IntOperand" value="999"/>
    </expression>
  </effects>
  <powers id="Shotgun" effects="//@effects.0">
    <target xsi:type="ballandpaddle:ObjectTarget" item="//@balls.0"/>
  </powers>
</ballandpaddle:Root>
